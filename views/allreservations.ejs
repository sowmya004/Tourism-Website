<%- include("partials/header") -%>
<h1 class="headings"> Reservations </h1>
<head>
    <style>
    table {
      font-family: arial, sans-serif;
      border-collapse: collapse;
      width: 100%;
    }
    td, th {
      border: 1px solid #dddddd;
      text-align: left;
      padding: 8px;
    }
    tr:nth-child(even) {
      background-color: #dddddd;
    }
    </style>
    </head> 
<body>
    <table>
        <tr>
          <th>HotelName</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Phone no.</th>
          <th>Email ID</th>
          <th>Checkin Date</th>
          <th>No.of nights</th>
          <th>No.of rooms</th>
          <th>No.of adults</th>
          <th>No.of children</th>
        </tr>
 <%  displays.forEach(function(display){ %>
    <tr>
        <td><%= display.hotel_name %></td>
        <td><%= display.firstname %></td>
        <td><%= display.lastname %></td>
        <td><%= display.phone %></td>
        <td><%= display.email %></td>
        <td><%= display.date %></td>
        <td><%= display.nights %></td>
        <td><%= display.rooms %></td>
        <td><%= display.adults %></td>
        <td><%= display.children %></td>
      </tr>
      <% }) %>
    </table>
</body>
           

<%- include("partials/footer") -%>








